<%= form_for @credential, url: users_path, html: {autocomplete: :off} do |form| %>
form_for(@article, url: articles_path)
  <% if @credential.errors.any? %>
    <div class="error_messages">
      <h2>Form is invalid</h2>
      <ul>
        <% for message in @credential.errors.full_messages %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div><!--.error_messages-->
  <% end %>
<div class="row">
  <div class="small-8 columns">
    <%= form.label :email, class: "small-6 columns"%>
    <%= form.text_field(:email, class: "small-6 columns") %>
  </div>
</div>
  <div class="row">
    <div class="small-8 columns">
      <%= form.label :password, class: "small-6 columns"%>
      <%= form.text_field(:password, class: "small-6 columns") %>
    </div>
  </div>
  <div class="row">
    <div class="small-8 columns">
      <%= form.label :password_confirmation, class: "small-6 columns"%>
      <%= form.text_field(:password_confirmation, class: "small-6 columns") %>
    </div>
  </div>       
  <div><%= form.submit "Register", :class => "small button" %></div>
<% end %>